{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "require-jsdoc": "off",
        "no-invalid-this": "off",
        "no-unused-vars": "warn",
        "comma-dangle": "off",
        "guard-for-in": "off",
        "react/prop-types": "off"
    }
}
